{
    "manifest_version": 3,    
    "name": "tab mute extension",
    "version": "1.0",
    "description": "tab mute extension description",

    "action": {
        "default_popup": "popup/popup.html"
    },

    "background": {
        "service_worker": "src/background.js"
    },

    "permissions": [
        "tabs"
    ]
}